package com.ob.helloworld.demo;

public class Datatypes {
	public static void main(String[] args) {
		byte b1 = 1;
		short s1 = 2;
		int i1 = 3;
		long l1 = 4;
		float f1 = 6;
		double d1 = 8;
		
		//implicit
		
		double d2 = f1;
		float f2 =l1;
		long l2 = i1;
		int i2 = s1;
		short s2 = b1;
	
		System.out.println("Implicit");
		System.out.println(d2 + f2 + l2 + i2 + s2);
		
	}

}
